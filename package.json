{
    "name": "email-viewer",
    "displayName": "Email Viewer",
    "description": "View EML files",
    "version": "1.0.0",
    "publisher": "TomasHubelbauer",
    "repository": {
        "type": "git",
        "url": "https://github.com/TomasHubelbauer/vscode-email-viewer.git"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "workspaceContains:**/*.eml",
        "onFileSystem:eml"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "@types/fs-extra": "^5.0.4",
        "@types/mailparser": "^2.0.2",
        "@types/pretty-bytes": "^5.1.0",
        "fs-extra": "^7.0.0",
        "mailparser": "^2.3.2",
        "pretty-bytes": "^5.1.0"
    }
}
